library(readxl)
dados <- read_excel("base de dados.xlsx")
head(dados)
preco <- dados$`Preço Mínimo`
print("Medidas de Tendência Central")
mean(preco)     # Média
median(preco)   # Mediana
print("Medidas de Dispersão")
min(preco)           # Valor mínimo
max(preco)           # Valor máximo
range(preco)         # Mínimo e máximo
diff(range(preco))   # Amplitude total
var(preco)           # Variância
sd(preco)            # Desvio padrão
sd(preco)/mean(preco) * 100   # Coeficiente de variação (%)
print("Medidas Separatrizes")
quantile(preco, probs = c(0.25, 0.5, 0.75))  # Quartis
quantile(preco, probs = seq(0.1, 0.9, 0.1))  # Decis
print("Análise gráfica.")
boxplot(preco,
main = "Boxplot do Preço Mínimo",
ylab = "Preço (R$)",
col = "lightgreen")
